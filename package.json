{
  "name": "express-template",
  "version": "1.0.0",
  "description": "Express API 模板项目",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "apidoc": "apidoc -i src/ -o src/public/apidoc/",
    "generate": "npx drizzle-kit generate",
    "migrate": "npx drizzle-kit migrate",
    "push": "npx drizzle-kit push",
    "database-push": "npm run generate && npm run migrate && npm run push"
  },
  "keywords": [
    "express",
    "api",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@vercel/blob": "^1.1.1",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.4.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.0",
    "drizzle-orm": "^0.43.1",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "jsdom": "^26.1.0",
    "json5": "^2.2.3",
    "jsonrepair": "^3.12.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.8.2",
    "prettier": "^3.5.3",
    "qiniu": "^7.10.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "apidoc": "^0.29.0",
    "nodemon": "^3.0.2"
  }
}
